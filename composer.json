{
	"name": "webchemistry/task-runner",
	"require": {
		"php": ">=8.1",
		"nette/di": "^3.0",
		"utilitte/asserts": "~v1.3.0",
		"psr/log": "^1.0 || ^2.0 || ^3.0"
	},
	"autoload": {
		"psr-4": {
			"WebChemistry\\TaskRunner\\": "src"
		}
	},
	"require-dev": {
		"phpstan/phpstan": "^1.7",
		"tracy/tracy": "^2.9",
		"phpstan/phpstan-nette": "^1.0",
		"phpstan/extension-installer": "^1.1",
		"symfony/console": "^6.1"
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
